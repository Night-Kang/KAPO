/* pages/feedback/feedback.wxss */
.Rose-fixed {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  border-top: 1px solid #d9d9d9;
  background: #fff;
  z-index: 5
}
.Rose-flex {
  padding: 14rpx 20rpx;
  display: flex;
}
.Rose-none {
  display: block;
  flex: none;
  width: 80rpx;
}
.Rose-names {
  font-size: 28rpx;
  color: #666;
}
.Rose-times {
  padding: 16rpx 0;
  color: #999;
  font-size: 20rpx;
}
.Rose-edit {
  width: 20%;
  float: left;
}
.Rose-edit icon {
  font-size: 36rpx;
  margin: 0 10rpx;
  color: #999;
}
.Rose-empty {
  padding: 40rpx 0;
  text-align: center;
  font-size: 14px;
  color: #999;
}
.iconfont.icon-kafei {
  display: block;
  font-size: 90rpx;
  color: #eee;
  margin-bottom: 10rpx;
}
.Rose-small {
  padding: 0 10rpx;
  font-size: 30rpx;
  color: #999;
}
.Rose-back {
  margin: 20rpx 0 0 20rpx;
  color: #999;
  font-size: 28rpx;
  padding: 10rpx;
  line-height: 1.6;
  background: #f5f5f5;
}
.Rose-tt {
  color: #333;
}
.Rose-back-item {
  line-height: 1.4;
  word-break: break-all;
  padding: 5rpx 0;
}
.Rose-image {
  flex: none;
  width: 70rpx;
  height: 70rpx;
}
.Rose-resource {
  display: flex;
}
.Rose-comments{
  background: #fff;
  margin:20rpx 0 0;
}
.Rose-lists {
  padding: 20rpx;
  border-top: 1rpx solid #f2f2f2;
  border-bottom: 1rpx solid #f2f2f2;
  background: #fff;
}
.Rose-feeds {
  padding: 10px;
  border-bottom: 1rpx solid #f2f2f2;
  font-size: 32rpx;
  color: #666;
}
.Rose-edit {
  width: 20%;
  float: left;
}
.Rose-edit icon {
  font-size: 36rpx;
  margin: 0 10rpx;
  color: #999;
}
.Rose-items {
  padding: 30rpx 0;
  border-bottom: 1rpx solid #f2f2f2;
}
.Rose-items:last-child{
  border-bottom:none;
}
.Rose-comment-title {
  line-height: 2;
  color: #333;
  font-size: 16px;
}
.Rose-comment-content {
  color: #666;
  font-size: 32rpx;
  line-height: 44rpx;
}
.Rose-user {
  flex: 1;
  font-size: 24rpx;
  color: #a8a8a8;
  line-height: 1;
}
/*改版评论*/
.Rose-none .iconfont.icon-hongbao1 {
  color: #ff80c2;
}
.Rose-input {
  flex: 1;
  padding: 8rpx 20rpx;
  background: #f5f5f5;
  border: 2rpx solid #d9d9d9;
  line-height: 1;
  border-radius: 4rpx;
  font-size: 30rpx;
}
.Rose-post {
  font-size:24rpx;
  margin:0 0 0 20rpx;
  height: 70rpx;
  line-height:70rpx;
  width:auto;
  background: #4bb777;
  color: #fff
}
button[disabled]:not([type]) {
  background: #d9d9d9;
}
